{
  "name": "theminglayer",
  "version": "0.0.11",
  "description": "ThemingLayer",
  "keywords": [
    "design tokens",
    "theming",
    "css",
    "variables",
    "custom properties",
    "postcss",
    "tailwind"
  ],
  "homepage": "https://www.theminglayer.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/universse/theminglayer.git",
    "directory": "packages/theminglayer"
  },
  "license": "See LICENSE.md",
  "author": "Si Phuoc <phuoc317049@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./postcss": {
      "types": "./dist/postcss.d.ts",
      "import": "./dist/postcss.js",
      "require": "./postcss.cjs"
    },
    "./plugins": {
      "types": "./dist/plugins.d.ts",
      "import": "./dist/plugins.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.js"
    },
    "./browser/build": {
      "types": "./dist/browser/build.d.ts",
      "import": "./dist/browser/build.js"
    },
    "./browser/plugins": {
      "types": "./dist/browser/plugins.d.ts",
      "import": "./dist/browser/plugins.js"
    }
  },
  "browser": {
    "esbuild": false,
    "fast-glob": false,
    "glob-parent": false,
    "json5": false,
    "micromatch": false,
    "node:fs": false,
    "node:fs/promises": false,
    "node:module": false,
    "node:path": false,
    "node:url": false,
    "slash": false,
    "~/utils/node": false,
    "~/utils/importJs": false
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "theminglayer": "./dist/cli.js"
  },
  "files": [
    "dist",
    "postcss.cjs"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "size": "pnpm build && size-limit",
    "test": "glob -c \"node --import tsx --test\" \"__tests__/**/*.test.ts\"",
    "test:watch": "glob -c \"node --import tsx --test --watch\" \"__tests__/**/*.test.ts\""
  },
  "dependencies": {
    "@csstools/selector-specificity": "4.0.0",
    "@humanwhocodes/momoa": "3.2.0",
    "cac": "6.7.14",
    "chokidar": "3.6.0",
    "clsx": "2.1.1",
    "culori": "4.0.1",
    "destr": "2.0.3",
    "esbuild": "0.23.1",
    "fast-glob": "3.3.2",
    "fastest-levenshtein": "1.0.16",
    "glob-parent": "6.0.2",
    "import-from-esm": "1.3.4",
    "js-yaml": "4.1.0",
    "json5": "2.2.3",
    "kleur": "4.1.5",
    "klona": "2.0.6",
    "lodash.groupby": "4.6.0",
    "lodash.setwith": "4.3.2",
    "micromatch": "4.0.8",
    "neotraverse": "0.6.18",
    "postcss-selector-parser": "6.1.2",
    "slash": "5.1.0",
    "ts-pattern": "5.3.1",
    "valibot": "0.41.0",
    "zipson": "0.2.12",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "11.1.4",
    "@theminglayer/design-tokens": "workspace:*",
    "@types/glob-parent": "5.1.3",
    "@types/js-yaml": "4.0.9",
    "@types/lodash.groupby": "4.6.9",
    "@types/lodash.setwith": "4.3.9",
    "@types/micromatch": "4.0.9",
    "@types/node": "22.5.3",
    "glob": "11.0.0",
    "npm-run-all": "4.1.5",
    "pattycake": "0.0.2",
    "postcss": "8.4.45",
    "size-limit": "11.1.4",
    "tsup": "8.2.4",
    "tsx": "4.19.0",
    "type-fest": "4.26.0",
    "typescript": "5.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
